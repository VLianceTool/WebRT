<Platform name="Web_Emsc">


<exe>
	<Preload>wType/CreateConfig.exe</Preload>
	<ViewIn>wType/GetBrowsers.exe</ViewIn>
	
          <Compiler>{vWebRT_Python}python.exe "{vWebRT_Emsc}em++" --em-config "{vWebRT_Config}" --cache "{wCompiler_Dir}_cache/"</Compiler>
	        <Linker>{vWebRT_Python}python.exe "{vWebRT_Emsc}em++" --em-config "{vWebRT_Config}" --cache "{wCompiler_Dir}_cache/"</Linker>
     <Linker_Static>{vWebRT_Python}python.exe "{vWebRT_Emsc}em++" --em-config "{vWebRT_Config}" --cache "{wCompiler_Dir}_cache/"</Linker_Static>
    <Linker_Dynamic>{vWebRT_Python}python.exe "{vWebRT_Emsc}em++" --em-config "{vWebRT_Config}" --cache "{wCompiler_Dir}_cache/"</Linker_Dynamic>
	
    <Resource></Resource>
</exe>
<arguments>

	 <Compiler>-I"{vWebRT_Emsc}system/include/" </Compiler>

	  <CompilerLinker>--shell-file "{vWebRT_Shell}"</CompilerLinker> 
	  
	  <Finally_CopyFolder>{vWebRT_Shell} -> {wDir}{wOutBin} !> *.s_html</Finally_CopyFolder> 
	  
     <CompilerLinker>--emrun </CompilerLinker>
	 <CompilerLinker>--bind </CompilerLinker>
	
     <CompilerLinker>-fno-exceptions </CompilerLinker>
	 <CompilerLinker>-m32 </CompilerLinker>
	 <CompilerLinker>-fcolor-diagnostics -fansi-escape-codes </CompilerLinker>
	 <CompilerLinker>-std=c++11 </CompilerLinker>
	  
	 <Compiler>-MD </Compiler>
	 <Compiler>-I"{wDir}" </Compiler>

	<Linker>-Qunused-arguments -Wno-unused-value -Wno-deprecated-register -Wno-ignored-attributes -Qunused-arguments -Wno-ignored-pragmas </Linker>	

	<Linker>-Werror=return-type </Linker>
	

    <Linker>-static-libgcc -static-libstdc++ </Linker>
   <Linker_Static>-static-libgcc -static-libstdc++ </Linker_Static>
   <Linker_Dynamic>-static-libgcc -static-libstdc++ </Linker_Dynamic>
	  
   <Debug>-g -fstandalone-debug </Debug>
	
</arguments>
  
  <Website>https://clang.llvm.org/docs/UsersManual.html</Website>
</Platform>


